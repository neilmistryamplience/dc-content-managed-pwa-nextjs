    "settings":
    {
        "label": "Navigation",
        "visualizations": [
        {
            "label": "PWA (Deployed)",
            "templatedUri":"https://amp-pwa-harpercollins.vercel.app/visualization?id={{content.sys.id}}&contentApi={{vse.domain}}",
            "default": true
        },
        {
            "label": "PWA (Localhost)",
            "templatedUri":"http://localhost:3000/visualization?id={{content.sys.id}}&contentApi={{vse.domain}}",
